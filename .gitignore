# 🚀 WordPress Plugin GitIgnore for TermsToTax

# 💾 Ignore OS-generated files
.DS_Store
Thumbs.db

# 🔄 Ignore Composer dependencies (only if installed via Composer)
vendor/
composer.lock

# 🏗 Ignore Node.js & Build Files (if used in development)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# 📝 Ignore IDE-specific files
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# 🔥 Ignore logs & temp files
debug.log
wp-content/debug.log
*.log

# 📂 Ignore backup files
*.bak
*.swp
*.swo
*.swn

# 🔄 Ignore WordPress core files (if repo is in /wp-content/plugins/)
wp-admin/
wp-includes/

# 🚫 Ignore WordPress uploads & cache
wp-content/uploads/
wp-content/cache/

# 🚀 Ignore Local Environment Files
.env
.env.local
.env.*.local

# 📦 Ignore dependency manager files
package-lock.json
yarn.lock

# 🔧 Ignore PHPUnit test results (if using PHPUnit)
phpunit.xml
phpunit.xml.dist
tests/_output/
tests/_support/_generated/

# ⚙ Ignore WP-CLI config (if applicable)
wp-cli.local.yml
wp-cli.yml

# 🔍 Ignore Hidden System Files
.svn/
.cvs/
.hg/
.bzr/
